#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t,n,k,i,j,m,p,ni,ra;
    char T;
    while(cin>>t)
    {
    for(ni=0;ni<t;ni++)
    {
       // putchar('\n');
        cin>>k>>n;
        if(ni)cout<<endl;
        for(ra=0;ra<n;ra++)
        {
            if(ra)cout<<endl;
            for(i=1; i<=k; i++)
            {
                for(j=1; j<=i; j++)
                {
                    cout<<i;
                }
                cout<<endl;
                //putchar('\n');
            }
            for(m=k-1; m>=1; m--)
            {
                for(p=1; p<=m; p++)
                    cout<<m;
                    cout<<endl;
                //putchar('\n');
            }
           // if(n>0)
               // putchar('\n');
        }

    }
}
}
