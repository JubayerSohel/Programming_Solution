#include <iostream>
#include <string>
#include <algorithm>

using namespace std;

int main()
{
        string s;
        int t;
        cin>>t;
        cin.ignore(256,'\n');
        while(t--)
        {
        getline(cin, s);
        reverse(s.begin(), s.end());
        cout<<s;
        cout<<endl;
        }
        return 0;
}
