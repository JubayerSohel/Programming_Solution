#include<bits/stdc++.h>
using namespace std;
int main()
{
int a,b,c,t,i;
cin>>t;
for(i=1;i<=t;i++)
{
    cin>>a>>b>>c;
    if( b>a && a>c || c>a && a>b )
		{
        cout<<"Case "<<i<<":"<<" "<<a<<endl;
		}
		else if( a>b && b>c || c>b && b>a )
		{
        cout<<"Case "<<i<<":"<<" "<<b<<endl;
		}
		else
		if( a>c && c>b || b>c && c>a )
		{
        cout<<"Case "<<i<<":"<<" "<<c<<endl;
		}
}
}

