#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,l,p;
    string s;
    cin>>t;
    while(t--)
    {
        cin>>s;
        l=s.size();
        if(l==5)
            cout<<3<<endl;
        else if(l==3)
        {
            p=0;
            if(s[0]=='o')p++;
            if(s[1]=='n')p++;
            if(s[2]=='e')p++;
            if(p>=2)
                cout<<"1"<<endl;
            else
                cout<<"2"<<endl;
        }

    }
}
