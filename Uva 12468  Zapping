#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a,b,c;
    while(cin>>a>>b)
    {
        if(a==-1 && b==-1)
            break;
        else
        {
            if(a>b)
            {
                c=(a-b);
            }
            else
                c=b-a;
            if(c>50)
                c=100-c;
            cout<<c<<endl;
        }
    }
}
