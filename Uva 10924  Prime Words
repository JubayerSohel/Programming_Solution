#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i,r,p,j;
    string s;
    while(cin>>s)
    {
        r=0;
        p=0;
        for(i=0;i<s.size();i++)
        {
            if(s[i]=='a')
                r+=1;
            else if(s[i]=='b')
                r+=2;
            else if(s[i]=='c')
                r+=3;
            else if(s[i]=='d')
                r+=4;
            else if(s[i]=='e')
                r+=5;
            else if(s[i]=='f')
                r+=6;
            else if(s[i]=='g')
                r+=7;
            else if(s[i]=='h')
                r+=8;
            else if(s[i]=='i')
                r+=9;
            else if(s[i]=='j')
                r+=10;
            else if(s[i]=='k')
                r+=11;
            else if(s[i]=='l')
                r+=12;
            else if(s[i]=='m')
                r+=13;
            else if(s[i]=='n')
                r+=14;
            else if(s[i]=='o')
                r+=15;
            else if(s[i]=='p')
                r+=16;
            else if(s[i]=='q')
                r+=17;
            else if(s[i]=='r')
                r+=18;
            else if(s[i]=='s')
                r+=19;
            else if(s[i]=='t')
                r+=20;
            else if(s[i]=='u')
                r+=21;
            else if(s[i]=='v')
                r+=22;
            else if(s[i]=='w')
                r+=23;
            else if(s[i]=='x')
                r+=24;
            else if(s[i]=='y')
                r+=25;
            else if(s[i]=='z')
                r+=26;
            else if(s[i]=='A')
                r+=27;
            else if(s[i]=='B')
                r+=28;
            else if(s[i]=='C')
                r+=29;
            else if(s[i]=='D')
                r+=30;
            else if(s[i]=='E')
                r+=31;
            else if(s[i]=='F')
                r+=32;
            else if(s[i]=='G')
                r+=33;
            else if(s[i]=='H')
                r+=34;
            else if(s[i]=='I')
                r+=35;
            else if(s[i]=='J')
                r+=36;
            else if(s[i]=='K')
                r+=37;
            else if(s[i]=='L')
                r+=38;
            else if(s[i]=='M')
                r+=39;
            else if(s[i]=='N')
                r+=40;
            else if(s[i]=='O')
                r+=41;
            else if(s[i]=='P')
                r+=42;
            else if(s[i]=='Q')
                r+=43;
            else if(s[i]=='R')
                r+=44;
            else if(s[i]=='S')
                r+=45;
            else if(s[i]=='T')
                r+=46;
            else if(s[i]=='U')
                r+=47;
            else if(s[i]=='V')
                r+=48;
            else if(s[i]=='W')
                r+=49;
            else if(s[i]=='X')
                r+=50;
            else if(s[i]=='Y')
                r+=51;
            else if(s[i]=='Z')
                r+=52;
        }
        for(j=2;j<=r/2;j++)
        {
            if(r%j==0)
            {
                p++;
                break;
            }
        }
        if(p==0)
            cout<<"It is a prime word."<<endl;
        else
            cout<<"It is not a prime word."<<endl;
    }
}
