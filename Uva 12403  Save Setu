#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long int c,d,t;
    string s;
    cin>>t;
    d=0;
    while(t--)
    {
        cin>>s;
        if(s=="donate")
        {
            cin>>c;
            d+=c;
        }
        else if(s=="report")
            cout<<d<<endl;
    }
}
