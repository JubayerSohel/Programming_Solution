#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,t,i,j,ans,a;
    cin>>t;
    for(i=1;i<=t;i++)
    {
        cin>>n;
        ans=0;
        for(j=1;j<=n;j++)
        {
            cin>>a;
            if(a>ans)
                ans=a;
        }
        cout<<"Case "<<i<<": "<<ans<<endl;
    }
}
