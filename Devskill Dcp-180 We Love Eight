#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,l,res;
    string s;
    cin>>t;
    while(t--)
    {
        cin>>s;
        l=s.length();
        res=s[l-3]*100+s[l-2]*10+s[l-1];
        if(res%8==0)
        {
            cout<<"YES"<<endl;
        if(s[0]=='A')
            cout<<"1"<<endl;
            else
                cout<<"0"<<endl;
        }
        else
            cout<<"NO"<<endl;
    }
    return 0;
}
