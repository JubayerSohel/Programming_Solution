#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i,res,k=0,n;
    while(cin>>n)
    {
        if(n<0)
            break;
        else
        {
            for(i=0;i<n;i++)
            {
                if(pow(2,i)>=n)
                {
                    res=i;
                    break;
                }
            }
            k++;
            cout<<"Case "<<k<<":"<<" "<<res<<endl;
        }
    }
}
